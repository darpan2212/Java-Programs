package org.unordered.list;

public class List<T> {

	public List<T> () {

	}

	private class Node {
		private T value;
		private Node next;

		public T getValue() {
			return value;
		}

		public void setValue(T value) {
			this.value = value;
		}

		public Node getNext() {
			return next;
		}

		public void setNext(Node next) {
			this.next = next;
		}
	}

	private Node head = null;
	private Node tail = null;

	public boolean add(T e) {
		Node nd = new Node();
		nd.setValue(e);
		if(head==null){
			head = nd;
			tail = nd;
		}else{
			
		}
		return true;
	}
}