package org.ordered.list;

public class TestSortedList {

	public static void main(String[] args) {

		SortedLinkedList<Integer> si = new SortedLinkedList<Integer>();
		si.add(25);
		si.add(10);

		System.out.println("size -------- " + si.size());
		si.traverse();

		si.add(26);

		System.out.println("size -------- " + si.size());
		si.traverse();

		si.add(15);

		System.out.println("size -------- " + si.size());
		si.traverse();

		si.add(20);

		System.out.println("size -------- " + si.size());
		si.traverse();
		
		si.add(30);

		System.out.println("size -------- " + si.size());
		si.traverse();

	}

}