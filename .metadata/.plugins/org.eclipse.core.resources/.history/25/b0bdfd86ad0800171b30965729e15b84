package org.ordered.list;


public class SortedLinkedList<E>{

	private class Node<T> implements Comparable<T>{
		T value;
		Node<T> next;
		public T getValue() {
			return value;
		}
		public void setValue(T value) {
			this.value = value;
		}
		public Node<T> getNext() {
			return next;
		}
		public void setNext(Node<T> next) {
			this.next = next;
		}
		@Override
		public int compareTo(T o) {
			return o.toString().compareToIgnoreCase(this.getValue().toString());
		}
	}
	
	private Node<E> head;
	private Node<E> tail;
	
	public void add(E e){
		Node<E> nd = new Node<E>();
		nd.setValue(e);
		
		if(head == null){
			head = nd;
			tail = nd;
		}
	}
	
	
	public static void main(String[] args) {

		SortedLinkedList<String> sss= new SortedLinkedList<String>();
		
		sss.head.setValue("darpan");
		sss.head.setNext(sss.tail);
		sss.tail.setValue("Madlani");
		
		String s = sss.tail.getValue();
		
		System.out.println(sss.head.compareTo(s));
	}

}